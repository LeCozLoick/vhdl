restart -f
force -freeze sim:/parking/clk 1 0, 0 {50 ps} -r 100
force -freeze sim:/parking/nb_max 1000 0
force -freeze sim:/parking/sensor_in 0 0
force -freeze sim:/parking/sensor_out 0 0
force -freeze sim:/parking/reset 1 0
force -freeze sim:/parking/reset 0 100
force -freeze sim:/parking/ld_reg 1 200
force -freeze sim:/parking/ld_reg 0 300
 force -freeze sim:/parking/sensor_out 1 400
force -freeze sim:/parking/sensor_out 0 600
 run 10000
